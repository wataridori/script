print("Calculating the 20th Fibonacci Number ...")
start = time()
print("Start time : ",start)

def add (a,b) {
    a + b
}

def fib (n) {    
    if (n < 3) {
       1
    } else {
       add (fib (n-1),fib (n-2))
    }
}
result = fib(20)
end = time()
print("End time : ",end)
print("The result is : ",result)
print("Computation time : ",end-start, " second!")
print("Current time : ",date());