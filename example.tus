start = time()
print("Start time : ",start)

def add (a,b) {
    a + b
}

def fib (n) {    
    if (n < 3) {
       1
    } else {
       add (fib (n-1),fib (n-2))
    }
}

end = time()
print("End time : ",end)

result = fib(5)
print("The result is : ",result)
print("Computation time : ",end-start, " second!")